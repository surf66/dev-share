{
  "name": "dev-share",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "start:server": "nodemon .",
    "start": "concurrently --kill-others  \"webpack-dev-server --content-base web/\" \"nodemon .\" \"npm run watch-css\"",
    "posttest": "npm run lint && nsp check",
    "build": "webpack",
    "build-css": "node-sass-chokidar client/sass -o ./web/css",
    "watch-css": "npm run build-css && node-sass-chokidar client/sass -o ./web/css --watch --recursive"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "compression": "^1.0.3",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.2.0",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "jquery": "^3.2.1",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^5.0.0",
    "loopback-connector-mongodb": "^1.18.1",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "nsp": "^2.1.0"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "dev-share"
}
